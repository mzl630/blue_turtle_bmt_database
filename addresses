CREATE TABLE `blueturtle_bmt_database`.`addresses` (
  `id` INT UNSIGNED NOT NULL AUTO_INCREMENT,
  `created_at` DATETIME NULL,
  `updated_at` DATETIME NULL,
  `entity_template_id` DECIMAL(38,0) NOT NULL,
  `deleted` TINYINT(1) UNSIGNED NULL DEFAULT 0,
  `st_line_1` VARCHAR(45) NULL,
  `st_line_2` VARCHAR(45) NULL,
  `city` VARCHAR(45) NULL,
  `state` CHAR(2) NULL,
  `country` CHAR(2) NULL,
  `postal` VARCHAR(10) NULL,
  `home_phone` VARCHAR(20) NULL,
  `cell_phone` VARCHAR(20) NULL,
  `alt_phone` VARCHAR(20) NULL,
  `fax` VARCHAR(20) NULL,
  `email` VARCHAR(50) NULL,
  PRIMARY KEY (`id`),
  KEY `i_addresses_entity_template_id` (`entity_template_id` ASC))
  ENGINE = InnoDb DEFAULT CHARSET=utf8;
