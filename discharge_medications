CREATE TABLE `blueturtle_bmt_database`.`discharge_medicines` (
`id` DECIMAL(38,0) NOT NULL,
`created_at` DATETIME NULL,
`updated_at` DATETIME NULL,
`entity_template_id` DECIMAL(38,0) NULL,
`deleted` DECIMAL(1,0) NULL DEFAULT 0,
`medicine` VARCHAR(255) NULL,
`instruction` VARCHAR(255) NULL,
`other_specify` VARCHAR(255) NULL,
PRIMARY KEY (`id`))
ENGINE = InnoDB;
