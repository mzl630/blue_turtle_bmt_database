CREATE TABLE `blueturtle_bmt_database`.`discharge_summaries` (
`id` INT UNSIGNED NOT NULL AUTO_INCREMENT,
`created_at` DATETIME NULL,
`updated_at` TIMESTAMP NULL,
`entity_template_id` DECIMAL(38,0) NULL,
`deleted` TINYINT(1) UNSIGNED NOT NULL DEFAULT 0,
`central_line_placement_date` DATE NULL,
`central_line_placement_surgeon` VARCHAR(50) NULL,
`deceased_at_discharge` ENUM('yes', 'no') NULL,
`major_problems` MEDIUMBLOB NULL,
`bma_indicator` ENUM('yes', 'no') NULL,
`bma_date` DATE NULL,
`bma_results` VARCHAR(255) NULL,
`last_pentamidine_indicator` ENUM('yes', 'no') NULL,
`last_pentamidine_date` DATE NULL,
`last_ivig_indicator` ENUM('yes', 'no') NULL,
`last_ivig_date` DATE NULL,
`discharge_meds` ENUM('yes', 'no') NULL,
`followup_date` DATE NULL,
`followup_with` VARCHAR(50) NULL,
`discharge_instructions` MEDIUMBLOB NULL,
`education_meeting_date` DATE NULL,
`send_date` DATE NULL,
PRIMARY KEY (`id`))
ENGINE = InnoDB;
