CREATE TABLE `blueturtle_bmt_database`.`discharge_summaries` (
`id` DECIMAL(38,0) NOT NULL,
`created_at` VARCHAR(45) NULL,
`updated_at` VARCHAR(45) NULL,
`entity_template_id` DECIMAL(38,0) NULL,
`deleted` DECIMAL(1,0) NULL DEFAULT 0,
`central_line_placement` VARCHAR(255) NULL,
`central_line_placement_surgeon` VARCHAR(255) NULL,
`deceased_at_discharge` DECIMAL(1,0) NULL,
`major_problems` MEDIUMBLOB NULL,
`bma_indicator` DECIMAL(1,0) NULL,
`bma_date` DATETIME NULL,
`bma_results` VARCHAR(255) NULL,
`last_pentamidine_indicator` DECIMAL(1,0) NULL,
`last_pentamidine_date` DATETIME NULL,
`last_ivig_indicator` DECIMAL(1,0) NULL,
`last_ivig_date` DATETIME NULL,
`discharge_meds` DECIMAL(1,0) NULL,
`followup_date` DATETIME NULL,
`followup_with` DATETIME NULL,
`discharge_instructions` MEDIUMBLOB NULL,
`education_meeting_date` DATETIME NULL,
`send_date` DATETIME NULL,
PRIMARY KEY (`id`))
ENGINE = InnoDB;
