CREATE TABLE `blueturtle_bmt_database`.`disease_diagnostics` (
`id` INT UNSIGNED NOT NULL AUTO_INCREMENT,
`created_at` DATETIME NULL,
`updated_at` DATETIME NULL,
`entity_template_id` DECIMAL(38,0) NULL,
`deleted` TINYINT(1) UNSIGNED NOT NULL DEFAULT 0,
 `status` VARCHAR(50) NULL,
 `diagnosis_date` DATE NULL,
 `reason` VARCHAR(255) NULL,
 `reason_specify` VARCHAR(255) NULL,
 `relapse_remission_comments` VARCHAR(255) NULL,
 `status_at_tx` VARCHAR(20) NULL,
 `status_at_tx_other` VARCHAR(50) NULL,
 `ex_site_cns` TINYINT(1) UNSIGNED NULL,
 `ex_site_cns_specify` VARCHAR(50) NULL,
 `ex_site_cns_testicular` TINYINT(1) UNSIGNED NULL,
 `ex_site_cns_testicular_specify` VARCHAR(50) NULL,
 `ex_site_cns_other` TINYINT(1) UNSIGNED NULL,
 `ex_site_cns_other_specify` VARCHAR(50) NULL,
 `ex_site_other` TINYINT(1) UNSIGNED NULL,
 `ex_site_specify` VARCHAR(50) NULL,
 `bm` DATE NULL,
 `csf` DATE NULL,
 `other` DATE NULL,
 `other_specify` VARCHAR(50) NULL,
 `morphology` TINYINT(1) UNSIGNED NULL,
 `morphology_value` DECIMAL(3,2) UNSIGNED NULL,
 `cytogenetics`TINYINT(1) UNSIGNED NULL,
 `cytogenetics_value` DECIMAL(3,2) UNSIGNED NULL,
 `mrd` TINYINT(1) UNSIGNED NULL,
 `mrd_value` DECIMAL(3,2) NULL,
 `molecular` TINYINT(1) UNSIGNED NULL,
 `molecular_value` DECIMAL(3,2) UNSIGNED NULL,
 `bcr_abl` TINYINT(1) UNSIGNED NULL,
 `bcr_abl_value` DECIMAL(3,2) UNSIGNED NULL,
 `other_evaluation` TINYINT(1) UNSIGNED NULL,
 `other_evaluation_value` DECIMAL(3,2) UNSIGNED NULL,
 `status_confirmed` DATE NULL,
 `email_comment` VARCHAR(255) NULL,
 PRIMARY KEY (`id`),
 KEY `i_disease_diagnostics_entity_template_id` (`entity_template_id` ASC))
 ENGINE = InnoDB DEFAULT CHARSET=utf8;
