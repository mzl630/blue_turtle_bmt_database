CREATE TABLE `blueturtle_bmt_database`.`donor_selections` (
  `id` DECIMAL(38,0) NOT NULL,
  `created_at` DATETIME NULL,
  `updated_at` DATETIME NULL,
  `entity_template_id` DECIMAL(38,0) NULL,
  `deleted` DECIMAL(1,0) NULL DEFAULT 0,
  `selection_status` VARCHAR(255) NULL,
  `donor_source` VARCHAR(255) NULL,
  `relationship` VARCHAR(255) NULL,
  `comments` MEDIUMBLOB NULL,
  `donor_match_grade` VARCHAR(255) NULL,
  `eligible` VARCHAR(255) NULL,
  `ineligible_idm` DECIMAL(1,0) NULL,
  `ineligible_idm_method` DECIMAL(1,0) NULL,
  `ineligible_idm_time` DECIMAL(1,0) NULL,
  `ineligible_idm_positive` DECIMAL(1,0) NULL,
  `ineligible_screening` DECIMAL(1,0) NULL,
  `ineligible_screening_history` DECIMAL(1,0) NULL,
  `ineligible_screening_assess` DECIMAL(1,0) NULL,
  `ineligible_screening_records` DECIMAL(1,0) NULL,
  `approved_on` DATE NULL,
  `cibmtr_sample_consent` VARCHAR(255) NULL,
  `cibmtr_sample_consent_date` DATE NULL,
  `consent` VARCHAR(255) NULL,
  `consent_date` DATE NULL,
  `blood_bank_by` VARCHAR(255) NULL,
  `blood_bank_on` DATE NULL,
  `pe` VARCHAR(255) NULL,
  `good_health` VARCHAR(255) NULL,
  `infectious_skin_lesions` VARCHAR(255) NULL,
  `drug_abuse` VARCHAR(255) NULL,
  `medications` MEDIUMBLOB NULL,
  `transfusion_history` MEDIUMBLOB NULL,
  `vaccinations` VARCHAR(255) NULL,
  `reviewed_by` VARCHAR(255) NULL,
  `questionnaire` VARCHAR(255) NULL,
  `signoff_date` DATE NULL,
  `suitability` VARCHAR(255) NULL,
  PRIMARY KEY (`id`))
  ENGINE = InnoDB;
