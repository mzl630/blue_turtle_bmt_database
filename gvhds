CREATE TABLE `blueturtle_bmt_database`.`gvhds` (
`id` DECIMAL(38,0) NOT NULL,
`created_at` DATETIME NULL,
`updated_at` DATETIME NULL,
`entity_template_id` DECIMAL(38,0) NULL,
`deleted` DECIMAL(1,0) NULL DEFAULT 0,
`gvhd_result` VARCHAR(255) NULL,
`onset_date` DATETIME NULL,
`skin_stage` DECIMAL(38,0) NULL,
`gut_stage` DECIMAL(38,0) NULL,
`liver_stage` DECIMAL(38,0) NULL,
`overall_grade` DECIMAL(38,0) NULL,
`chronic` DECIMAL(38,0) NULL,
PRIMARY KEY (`id`))
ENGINE = InnoDb;
