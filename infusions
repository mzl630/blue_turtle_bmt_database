CREATE TABLE `blueturtle_bmt_database`.`infusions` (
`id` DECIMAL(38,0) NOT NULL,
`created_at` DATETIME NULL,
`updated_at` DATETIME NULL,
`entity_template_id` DECIMAL(38,0) NULL,
`deleted` DECIMAL(1,0) NULL DEFAULT 0,
`infusion_type` VARCHAR(255) NULL,
`infusion_location` VARCHAR(255) NULL,
`utn` DECIMAL(38,0) NULL,
`infusion_date` DATETIME NULL,
`comments` VARCHAR(255) NULL,
`completed` DECIMAL(1,0) NULL,
`physician` VARCHAR(255) NULL,
`nurse` VARCHAR(255) NULL,
`started_by` VARCHAR(255) NULL,
`start_at` DATETIME NULL,
`ended_by` VARCHAR(255) NULL,
`end_at` DATETIME NULL,
`method` VARCHAR(255) NULL,
`volume` DECIMAL(38,0) NULL,
`pre_medication` VARCHAR(255) NULL,
`engraftment_type` VARCHAR(255) NULL,
`date_anc500` DATETIME NULL,
`date_anc1000` DATETIME NULL,
`date_platelets_20` DATETIME NULL,
`date_platelets_50` DATETIME NULL,
`mucositis_grade` VARCHAR(255) NULL,
`muscositis_grade_date` DATETIME NULL,
`vod` DECIMAL(1,0) NULL,
`vod_date` DATETIME NULL,
`vod_max_bili` VARCHAR(255) NULL,
`pulmonary_toxicity` DECIMAL(1,0) NULL,
`pulmonary_toxicity_date` DATETIME NULL,
`renal_insufficiency` DECIMAL(1,0) NULL,
`renal_insufficiency_date` DATETIME NULL,
`hypertension` DECIMAL(1,0) NULL,
`hypertension_date` DATETIME NULL,
`organ_impairment` DECIMAL(1,0) NULL,
`organ_impairment_date` DATETIME NULL,
`organ_impairment_specify` VARCHAR(255) NULL,
PRIMARY KEY (`id`))
ENGINE = InnoDb;
