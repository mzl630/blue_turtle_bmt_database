CREATE TABLE `blueturtle_bmt_database`.`infusions` (
`id` INT UNSIGNED NOT NULL AUTO_INCREMENT,
`created_at` DATETIME NULL,
`updated_at` TIMESTAMP NULL,
`entity_template_id` DECIMAL(38,0) NULL,
`deleted` TINYINT(1) UNSIGNED NOT NULL DEFAULT 0,
`infusion_type` VARCHAR(50) NULL,
`infusion_location` VARCHAR(50) NULL,
`utn` DECIMAL(38,0) NULL,
`infusion_date` DATE NULL,
`comments` VARCHAR(255) NULL,
`completed` TINYINT(1) UNSIGNED NULl,
`physician` VARCHAR(100) NULL,
`nurse` VARCHAR(100) NULL,
`started_by` VARCHAR(50) NULL,
`start_at` DATETIME NULL,
`ended_by` VARCHAR(50) NULL,
`end_at` DATETIME NULL,
`method` VARCHAR(20) NULL,
`volume` INT(10) UNISGNED NULL,
`pre_medication` VARCHAR(100) NULL,
`engraftment_type` VARCHAR(50) NULL,
`date_anc500` DATE NULL,
`date_anc1000` DATE NULL,
`date_platelets_20` DATE NULL,
`date_platelets_50` DATE NULL,
`mucositis_grade` VARCHAR(10) NULL,
`muscositis_grade_date` DATE NULL,
`vod` TINYINT(1) UNSIGNED NULL,
`vod_date` DATE NULL,
`vod_max_bili` DECIMAL(3,1) NULL,
`pulmonary_toxicity` TINYINT(1) UNSIGNED NULL,
`pulmonary_toxicity_date` DATE NULL,
`renal_insufficiency` TINYINT(1) UNSIGNED NULL,
`renal_insufficiency_date` DATE NULL,
`hypertension` TINYINT(1) UNSIGNED NULL,
`hypertension_date` DATE NULL,
`organ_impairment` TINYINT(1) UNSIGNED NULL,
`organ_impairment_date` DATE NULL,
`organ_impairment_specify` VARCHAR(50) NULL,
PRIMARY KEY (`id`))
ENGINE = InnoDb;
