CREATE TABLE `blueturtle_bmt_database`.`intial_insurance_approvals` (
`id` INT UNSIGNED NOT NULL AUTO_INCREMENT,
`created_at` DATETIME NULL,
`updated_at` DATETIME NULL,
`entity_template_id` DECIMAL(38,0) NULL,
`deleted` TINYINT(1) UNSIGNED NOT NULL DEFAULT 0,
`insurance_type` VARCHAR(255) NULL,
`participation` ENUM('participating', 'non_participating') NULL,
`lomn_requested_date` DATE NULL,
`lomn_recorded_date` DATE NULL,
`date_submit` DATE NULL,
`date_authorized` DATE NULL,
`contact_to_lomn` TINYINT(3) UNSIGNED NULL,
`lomn_to_submission` TINYINT(3) UNSIGNED NULL,
`submission_to_auth` TINYINT(3) UNSIGNED NULL,
`auth_to_typing` TINYINT(3) UNSIGNED NULL,
`comments` MEDIUMBLOB NULL,
`fc_comments` VARCHAR(255) NULL,
`transplant_outcome` VARCHAR(255) NULL,
PRIMARY KEY (`id`),
KEY `i_intial_insurance_approvals_entity_template_id` (`entity_template_id` ASC))
ENGINE = InnoDb DEFAULT CHARSET=utf8;
