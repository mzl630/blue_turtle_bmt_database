CREATE TABLE `blueturtle_bmt_database`.`patients` (
  `id` INT UNSIGNED NOT NULL AUTO_INCREMENT,
  `created_at` DATETIME NULL,
  `updated_at` DATETIME NULL,
  `entity_template_id` DECIMAL(38,0) NULL,
  `deleted` TINYINT(1) UNSIGNED NOT NULL DEFAULT 0,
  `last_hct_date` DATE NULL,
  `nmdp_id` VARCHAR(12) NULL,
  `ebmt_id` VARCHAR(20) NULL,
  `embmt_cic` VARCHAR(50) NULL,
  `team_id` VARCHAR(10) NULL,
  `crid` VARCHAR(50) NULL,
  `upn` DECIMAL(4,1) UNSIGNED NULL,
  `first_name` VARCHAR(50) NULL,
  `middle_name` VARCHAR(50) NULL,
  `last_name` VARCHAR(50) NULL,
  `date_of_birth` DATE NULL,
  `gender` ENUM('male','female') NULL,
  `ethnicity` VARCHAR(50) NULL,
  `parents` VARCHAR(255) NULL,
  `ssn` VARCHAR(255) NULL,
  `mrn` VARCHAR(10) NULL,
  `international` TINYINT(1) UNSIGNED NULL,
  `fert` TINYINT(1) UNSIGNED NULL,
  `deceased_date` DATE NULL,
  PRIMARY KEY (`id`),
  KEY `I_PATIENTS_ENTITY_TEMPLATE_ID` (`entity_template_id` ASC))
ENGINE = InnoDB DEFAULT CHARSET=utf8;
