CREATE TABLE `blueturtle_bmt_database`.`races` (
`id` DECIMAL(38,0) NOT NULL,
`created_by` DATETIME NULL,
`updated_by` DATETIME NULL,
`entity_template_id` DECIMAL(38,0) NULL,
`deleted` DECIMAL(1,0) NULL DEFAULT 0,
`race_group` VARCHAR(255) NULL,
`race_detail` VARCHAR(255) NULL,
PRIMARY KEY (`id`))
ENGINE = InnoDb;
